{
    "name": "kseven/ip-tools",
    "type": "library",
    "description": "PHP IP Tools for manipulation with IPv4 and IPv6",
    "keywords": ["ip", "ipv4", "ipv6", "tools", "utilities", "compare", "match", "subnet", "mask"],
    "license": "MIT",
    "homepage": "https://github.com/k7brasil/php-ip-tools",
    "support": {
        "issues": "https://github.com/k7brasil/php-ip-tools/issues",
        "source": "https://github.com/k7brasil/php-ip-tools"
    },
    "authors": [
        {
            "name": "K'Seven",
            "email": "anjsuporte@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-bcmath": "*"
    },
    "autoload": {
        "psr-4": {
            "KSeven\\IPTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KSeven\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "phpspec/phpspec": "~2.1",
        "squizlabs/php_codesniffer": "~2.3"
    }
}
